<%- include('layout/header') %>

    <section>
      <h1>Search for signature</h1>
      <p>This is a small application to provide a searchable table to filter network signatures</p>
      <p>Click Application Name to see more information</p>
    </section>
    <section>
      <table id="signatures" class="table table-striped table-bordered">
        <thead>
          <tr>
            <th>Application Name</th>
            <th>Type</th>
            <th>Times</th>
            <th>Times</th>
            <th>Times</th>
            <th>Vendor</th>
            <th>Vendor website</th>
            <th>Network comsuption type</th>
            <th>Description</th>
          </tr>
        </thead>
        <tbody>
      <% signatures.map(function(signature, index){ %>
        <tr>
          <td><a href="/<%= index %>"><%= signature.slice(0,1)[0]  %></a></td>
          <% signature.slice(1,9).map(function(td, column){ %>
              <td><%- td %></td>
          <% }) %>
        </tr>
      <% }); %>
        </tbody>
      </table>
    </section>
<%- include('layout/footer') %>